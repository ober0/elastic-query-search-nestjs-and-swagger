{
    "name": "elastic-search-nesjs-and-swagger",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build && tsc",
        "seed": "ts-node -r tsconfig-paths/register ./prisma/seeds/seed.ts",
        "seed:deploy": "node dist/prisma/seeds/seed.js",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
        "dev": "nest start --watch",
        "prod": "node dist/src/main.js",
        "generate": "npx prisma generate --schema ./prisma/schema/",
        "migrate:dev": "npx prisma migrate dev --schema ./prisma/schema/",
        "migrate:prod": "npm run migrate:deploy",
        "migrate:deploy": "npx prisma migrate deploy --schema ./prisma/schema/",
        "reset": "npx prisma migrate reset --force --schema ./prisma/schema/",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@faker-js/faker": "^9.9.0",
        "@nestjs/common": "^11.0.1",
        "@nestjs/core": "^11.0.1",
        "@nestjs/platform-express": "^11.0.1",
        "@nestjs/swagger": "^11.2.3",
        "@prisma/client": "^6.16.3",
        "@stylistic/eslint-plugin-ts": "^4.4.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.2",
        "reflect-metadata": "^0.2.2"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.2.0",
        "@eslint/js": "^9.18.0",
        "@nestjs/cli": "^11.0.0",
        "@nestjs/schematics": "^11.0.0",
        "@types/express": "^5.0.0",
        "@types/jest": "^30.0.0",
        "@types/node": "^22.10.7",
        "@types/supertest": "^6.0.2",
        "eslint": "^9.18.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.2",
        "globals": "^16.0.0",
        "jest": "^30.0.0",
        "prettier": "^3.4.2",
        "prisma": "^6.16.3",
        "source-map-support": "^0.5.21",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.5",
        "ts-loader": "^9.5.2",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.20.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
